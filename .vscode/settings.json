{
    "python.languageServer": "Pylance", // ms-python.vscode-pylance
    "python.analysis.ignore": [
      "**/manage.py",
      "**/seetings.py",
      "**/urls.py"
    ],
    "python.defaultInterpreterPath": "venv/Script/python.exe",
    "python.testing.unittestEnabled": false, // ms-python.python
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [], // -x to bail
    "python.linting.flake8Enabled": true,
    "python.linting.ignorePatterns": [
      "**/site-packages/**/*.py",
      ".vscode/*.py",
      "**/settings.py",
      "**/urls.py"
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "python.poetryPath": "poetry",
    "python.pipenvPath": "pipenv",
    "python.linting.flake8Args": [],
    "python.linting.mypyEnabled": true,
    "python.linting.pylintArgs": [
      "--load-plugins=pylint_django",
      "--errors-only"
    ],
    "cSpell.ignorePaths": [
      "package-lock.json",
      "node_modules",
      "vscode-extension",
      ".git/objects",
      ".vscode",
      ".vscode-insiders",
      "**/settings.py"
    ],
    "cSpell.words": [
      "xqyki"
    ],
}